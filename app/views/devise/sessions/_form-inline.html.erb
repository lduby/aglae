<%= form_for(resource, :as => resource_name, :html => {class:"form-inline login-form"}, :url => session_path(resource_name)) do |f| %>
  <%= f.email_field :email, :placeholder => "Email", :html => {class:"span2"} %>
  <%= f.password_field :password, :placeholder => "Password", :class => "input-medium" %>
  <%= f.submit "Sign in" %>
<% end %>